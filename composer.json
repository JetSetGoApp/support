{
    "name": "jetsetgoapp/support",
    "description": "Support package for JetSetGo application",
    "keywords": [
        "jetsetgo",
        "laravel",
        "support",
        "module",
        "konekt",
        "concord"
    ],
    "homepage": "https://github.com/jetsetgoapp/support",
    "authors": [
        {
            "name": "JetSetGo Team",
            "email": "developers@jetsetgo.com"
        }
    ],
    "require": {
        "konekt/concord": "^1.15"
    },
    "require-dev": {
        "tightenco/duster": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JetSetGo\\Support\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/duster fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JetSetGo\\Support\\SupportServiceProvider"
            ],
            "aliases": {
                "Supplier": "JetSetGo\\Support\\Facades\\Supplier"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
